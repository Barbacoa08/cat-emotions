{
  "name": "cat-emotions",
  "version": "0.1.0",
  "author": "Barbacoa08",
  "license": "MIT",
  "repository": "https://github.com/Barbacoa08/cat-emotions",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint ./src",
    "clean": "rm -rf coverage build",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage=true --watchAll=false",
    "test:badges": "yarn test:coverage && jest-coverage-badges --output ./badges",
    "test:watch": "react-scripts test",
    "netlify": "yarn && yarn build",
    "all": "yarn && yarn clean && yarn lint && yarn build && yarn test:badges"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.types.{ts,tsx}",
      "!src/reportWebVitals.ts",
      "!src/**/index.{ts,tsx}"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/strict",
      "prettier"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "chakra-react-select": "^3.1.1",
    "framer-motion": "^6",
    "netlify-identity-widget": "^1.9.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "recharts": "^2.1.9",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/netlify-identity-widget": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "eslint-config-prettier": "^8.5.0",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
